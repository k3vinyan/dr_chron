<tr>
  <td><img class="avatar" src="<%= patient['patient_photo'] %>"/></td>
  <td><%= patient['chart_id'] %></td>
  <td><%= patient['first_name'] %> <%= patient['last_name'] %></td>
  <td><%= patient['date_of_birth'] %></td>
  <td><%= patient['gender'] %></td>
  <td><%= patient['date_of_last_appointment'] %></td>
  <td>
    <% if patient['date_of_next_appointment'].blank? %>
      <%= link_to 'Schedule Appt', new_appointment_path(:patient_id => patient['id']) %>
    <% else %>
      <%= patient['date_of_next_appointment'] %>
    <% end %>
 </td>
</tr>


